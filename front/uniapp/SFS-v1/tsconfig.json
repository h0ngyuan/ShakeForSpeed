{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "types": [],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "vueCompilerOptions": {
    "globalTypesPath": "./shims-uni-app.d.ts"
  },
  "include": [
    "./pages/**/*.vue",
    "./main.js",
    "./api/**/*.js",
    "./components/**/*.vue",
    "./shims-vue.d.ts",
    "./App.vue"
  ],
  "exclude": [
    "node_modules",
    "unpackage"
  ]
}